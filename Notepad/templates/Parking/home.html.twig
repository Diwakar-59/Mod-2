{% extends "base.html.twig" %}

{% block body %}

{% block slots_available2 %} {{ slots.two_wheeler }} {% endblock %}
{% block slots_available4 %} {{ slots.four_wheeler }} {% endblock %}


{% block tickets_table %}
<tbody>
  {% for item in items %}
  <tr>
    <th> {{ item.slot_id }} </th>
    <td> {{ item.vehicle_number }} </td>
    <td> {{ item.entry_time }} </td>
    <td> {{ item.exit_time }} </td>
    {% if (item.exit_time == "") %}
      <td> {{ 'Occupied' }} </td>
    {% else %}
    <td> {{ 'Available' }} </td>
    {% endif %}

  </tr>
  {% endfor %}
  

</tbody> 
{% endblock %}






















{% endblock %}